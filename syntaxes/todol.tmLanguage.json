{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "TodoL",
  "patterns": [
    {
      "include": "#keywords"
    },
    {
      "include": "#strings"
    }
  ],
  "repository": {
    "keywords": {
      "patterns": [
        {
          "name": "keyword.operator.todol",
          "match": "\\[ \\]"
        },
        {
          "name": "comment",
          "match": "\\[(x|X)\\](.*)"
        }
      ]
    }
  },
  "scopeName": "TODO"
}
